{
  "hosting": {
    "public": "public",
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ]
  },
  "functions": {
    "predeploy": [
      "npm --prefix \"api\" run lint",
      "npm --prefix \"api\" run build"
    ],
    "source": "api",
    "runtime": "nodejs16"
  }
}
